<body class="d-flex flex-column vh-100" style="background-color: #e5eff7;">
    <nav class="navbar navbar-expand-lg  navbar-dark bg-dark sticky-top">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">CoffeeDelight</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup"
                aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav">
                    <a class="nav-link" href="/home">Home</a>
                    <a class="nav-link" href="/HotCoffee">Coffees</a>
                    <a class="nav-link" href="/donuts">Donuts</a>
                    <a class="nav-link" href="/update">Update</a>
                </div>
                <div class="navbar-nav" style="margin-left: auto;padding-right: 13px;">
                   
                         <a class="nav-link" href="/LogOut">Log Out</a>
                               
                </div>
            </div>
        </div>
    </nav>
    <section class="container-fluid py-3">
        <div class="row">
          <div class="col-4"></div>
            <div class="col-12 col-sm-4">
                <div class="card">
                    <img class="card-img-top " style="width: 100%;height: 250px;" src="https://images.unsplash.com/photo-1541167760496-1628856ab772?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=1337&q=80" alt="Card image cap">
                    <div class="card-body">
                      <h2 style="font-family:'Josefin Slab', serif;
                      " class="card-title"><%= coffee %> </h2>
                      <p class="card-text">You can add a review on our product here, this will help those who are new to our website.</p>
                    </div>
                    <ul class="list-group list-group-flush">
                      <form action="/review/<%= coffee %>" method="post" class="ValidForm" novalidate > 
                      <li class="list-group-item">
                        
                        <span style="font-family: 'Josefin slab', serif;font-size: 18px;">Rate This Coffee</span>
                        <span class="pt-1 starability-basic mb-1">
                            
                            <input type="radio" id="no-rate" class="input-no-rate" name="rating" value="0" checked aria-label="No rating." required/>
                            <input type="radio" id="first-rate1" name="rating" value="1" required/>
                            <label for="first-rate1" title="Terrible">1 star</label>
                            <input type="radio" id="first-rate2" name="rating" value="2" required/>
                            <label for="first-rate2" title="Not good">2 stars</label>
                            <input type="radio" id="first-rate3" name="rating" value="3" required/>
                            <label for="first-rate3" title="Average">3 stars</label>
                            <input type="radio" id="first-rate4" name="rating" value="4" required/>
                            <label for="first-rate4" title="Very good">4 stars</label>
                            <input type="radio" id="first-rate5" name="rating" value="5" required/>
                            <label for="first-rate5" title="Amazing">5 stars</label>
                        </span>
                          
                      
                          <label style="font-family: 'Josefin slab', serif;font-size: 18px;" class="form-label" for="reviewbody">Type In Your Review</label>
                          <textarea class="form-control" placeholder="What did you loved/hated about this coffee?" id="reviewbody" name="reviewbody" required></textarea>
                          <div class="text-center mt-2"><button class="btn px-3 btn-primary">Submit Review</button></div>
                      </li>
                      </form>
                    </ul>
                    
                  </div>
            </div>
            <div class="col-4"></div>
        </div>
    </section>


    <footer class="footer bg-dark py-2 mt-auto">
        <div class="container">
            <span class="text-muted">&copy; CoffeeDelight 2021</span>
        </div>
    </footer>

    <script>
        (function () {
            'use strict'


            const forms = document.querySelectorAll('.ValidForm')


            Array.prototype.slice.call(forms)
                .forEach(function (form) {
                    form.addEventListener('submit', function (event) {
                        if (!form.checkValidity()) {
                            event.preventDefault()
                            event.stopPropagation()
                        }

                        form.classList.add('was-validated')
                    }, false)
                })
        })()
    </script>
